{"intents": [
        {"tag": "greeting",
         "patterns": ["Hi",
                      "How are you", "help me",
                      "Is anyone there?",
                      "Hello",
                      "Good day"],
         "responses": ["Hello, thanks for visiting",
                       "Good to see you again",
                       "Hi there, how can I help?"],
         "context_set": ""
        },
        {"tag": "help",
         "patterns": ["can you help me",
                      "I need some information",
                      "I'm confused"],
         "responses": ["Sure, what can I do for you ?"],
         "context_set": ""
        },
        {"tag": "goodbye",
         "patterns": ["Bye",
                      "See you later",
                      "Goodbye"],
         "responses": ["See you later, thanks for visiting",
                        "Have a nice day",
                        "Bye! Come back again soon."]
        },
        {"tag": "thanks",
         "patterns": ["Thanks",
                      "Thank you",
                      "That's helpful"],
         "responses": ["Happy to help!",
                        "Any time!",
                        "My pleasure"]
        },
        {"tag": "arrdep",
         "patterns": ["what time",
                      "what time does flight land",
                      "arrived yet",
                      "departed yet",
                      "when does",
                      "when does the flight from arrive" ],
         "responses": ["Please check the arrival and departures page"]
        },
        {"tag": "timetable",
         "patterns": ["Do you fly to",
                      "When is the first flight to",
                      "Is there a flight to at",
                      "when is the first last flight to "],
         "responses": ["Please check the timetable page"]
        },
        {"tag": "packages",
         "patterns": ["I want to go on holiday",
                      "car hire",
                      "Do you sell package holidays",
                      "I want a hotel room in",
                      "I want to stay in"],
         "responses": ["We do great deals on many travel products when you buy them with a flight",
                        "Please make a flight and hotel booking"]
        },
        {"tag": "availability",
         "patterns": ["how much",
                      "I want a return flight from to",
                      "I want a one-way flight from to",
                      "Are there economy seats to",
                      "I want to fly club to",
                      "seats to please" ],
         "responses": ["You can find details on flight search page"]
        },
        {"tag": "payments",
         "patterns": ["I'd like to pay with",
                      "pay with",
                      "Do you take credit cards?",
                      "Do you accept Mastercard?",
                      "Are you cash only?" ],
         "responses": ["We accept VISA, Mastercard and AMEX",
                        "We accept most major credit cards"]
        }
   ]
}